/*******************************
    User Variable Overrides
*******************************/

@backgroundColor: @secondaryColor;
@borderRadius: 0;

@boxShadow: inset 0 -0.15em 0 0 fade(#000, 15);
@primaryBoxShadow: @boxShadow;
@secondaryBoxShadow: @boxShadow;
@coloredBoxShadow: @boxShadow;
@positiveBoxShadow: @boxShadow;
@negativeBoxShadow: @boxShadow;
@fontFamily: @headerFont;
